{% extends '::base.html.twig' %}

{% block body -%}

    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12">
            <h2><a href="{{ path('ticket') }}">
                 Ticket:
                </a>
                Vista
            </h2>
        </div>
    </div>

    {% for flashMessage in app.session.flashbag.get('mensaje') %}
        <h2 class="alert-success">{{ flashMessage }} </h2>
        <br /><br />
    {% endfor %}

    <div class="row">
        <div class="col-md-12">
            <div class="portlet light portlet-fit ">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-eye font-yellow"></i>
                        <span class="caption-subject font-yellow bold uppercase">Caracteristicas</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="general-item-list">
                        
                        <div class="item">
                            <div class="row">
                                <div class="col-xs-12 col-sm-2 col-md-2">
                                    Proyecto
                                </div>
                                <div class="col-xs-12 col-sm-3 col-md-3">
                                    {{ entity.proyecto }}
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="row">
                                <div class="col-xs-12 col-sm-2 col-md-2">
                                    Sistema Operativo:
                                </div>
                                <div class="col-xs-12 col-sm-3 col-md-3">
                                    {{ entity.sistemaOperativo }}
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="row">
                                <div class="col-xs-12 col-sm-2 col-md-2">
                                    Navegador:
                                </div>
                                <div class="col-xs-12 col-sm-3 col-md-3">
                                    {{ entity.navegador }}
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="row">
                                <div class="col-xs-12 col-sm-2 col-md-2">
                                    URL:
                                </div>
                                <div class="col-xs-12 col-sm-3 col-md-3">
                                    {{ entity.url }}
                                </div>
                            </div>
                        </div>
                        <div class="item">
                            <div class="row">
                                <div class="col-xs-12 col-sm-2 col-md-2">
                                    Descripcion:
                                </div>
                                <div class="col-xs-12 col-sm-3 col-md-3">
                                    {{ entity.descripcion }}
                                </div>
                            </div>
                        </div><br />

                        {{ form(delete_form) }}
                       
                    </div>
                </div>
            </div>
        </div>
    </div>
    
{% endblock %}
